class Launch
{
    public static void main(String[] args)
    {
        int a=10;
        int b=5;
        int c=15;
        
        if(a>b || b<c)
        {
            System.out.println("Condition is satisfied");
        }
        else
        {
           System.out.println("Condition is not satisfied"); 
        }
    }
}
